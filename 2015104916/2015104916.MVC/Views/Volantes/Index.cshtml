@model IEnumerable<_2015104916.Entities.Entities.Volante>

@{
    ViewBag.Title = "Volantes - Índice";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Índice</h2>

<p>
    @Html.ActionLink("Añadir", "Create")
</p>
<table class="table">
    <tr>
        <th>
          N° de Serie
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.NumSerie)
        </td>
        <td>
            @Html.ActionLink("Editar", "Edit", new { id=item.VolanteId }) |
            @Html.ActionLink("Detalles", "Details", new { id=item.VolanteId }) |
            @Html.ActionLink("Eliminar", "Delete", new { id=item.VolanteId })
        </td>
    </tr>
}

</table>
