@model IEnumerable<_2015104916.Entities.Entities.Propietario>

@{
    ViewBag.Title = "Propietarios - Índice";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Índice</h2>

<p>
    @Html.ActionLink("Añadir", "Create")
</p>
<table class="table">
    <tr>
        <th>
            N° Dni
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Nombres)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Apellidos)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Licencia)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Dni)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Nombres)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Apellidos)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Licencia)
        </td>
        <td>
            @Html.ActionLink("Editar", "Edit", new { id=item.PropietarioId }) |
            @Html.ActionLink("Detalles", "Details", new { id=item.PropietarioId }) |
            @Html.ActionLink("Eliminar", "Delete", new { id=item.PropietarioId })
        </td>
    </tr>
}

</table>
